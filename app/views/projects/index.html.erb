<h2>Projects Manager</h2>
<table id="container">
	<tr>
		<th>ID</th>
		<th>Title</th>
		<th>System</th>
		<th>Submitted</th>
		<th>Status</th>
		<th>Implemented</th>
		<th>Capital Cost</th>
		<th>Annual Electricity Savings</th>
		<th>Annual Gas Savings</th>
		<th>Annual Biomass Savings</th>
	</tr>
	<% @projects.each do |p| %>
		<tr>
			<td><%= p.id %></td>
			<td><%= p.title %></td>
			<td><%= p.system %></td>
			<td><%= p.date_submitted.strftime("%d %b %y") %></td>
			<td><%= p.status %></td>
			<td><%= p.date_implemented.strftime("%d %b %y") %></td>
			<td><%= p.capital_investment %></td>
			<td><%= p.annual_elec_savings %></td>
			<td><%= p.annual_gas_savings %></td>
			<td><%= p.annual_biomass_savings %></td>
			<td><%= link_to "Edit", edit_project_path(p) %></td>
		</tr>
	<% end %>
</table>
<h3><%= link_to "Add New Project", new_project_path %></h3>